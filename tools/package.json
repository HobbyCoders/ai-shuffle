{
  "name": "@ai-hub/tools",
  "version": "1.0.0",
  "type": "module",
  "description": "AI Hub tools for Claude code execution",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublish": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./image-generation": {
      "types": "./dist/image-generation/index.d.ts",
      "import": "./dist/image-generation/index.js"
    },
    "./image-generation/generateImage": {
      "types": "./dist/image-generation/generateImage.d.ts",
      "import": "./dist/image-generation/generateImage.js"
    },
    "./image-generation/editImage": {
      "types": "./dist/image-generation/editImage.d.ts",
      "import": "./dist/image-generation/editImage.js"
    },
    "./video-generation": {
      "types": "./dist/video-generation/index.d.ts",
      "import": "./dist/video-generation/index.js"
    },
    "./video-generation/generateVideo": {
      "types": "./dist/video-generation/generateVideo.d.ts",
      "import": "./dist/video-generation/generateVideo.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    }
  },
  "devDependencies": {
    "@types/node": "^24.10.2",
    "typescript": "^5.3.0"
  }
}
